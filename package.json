{
  "name": "ims-backend",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "main": "src/index.ts",
  "keywords": [],
  "scripts": {
    "commitlint": "commitlint --edit",
    "cspell": "cspell",
    "dev": "tsx --env-file=.env --watch .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "prisma migrate dev",
    "postmigrate": "pnpx @snaplet/seed sync",
    "prepare": "husky",
    "seed": "tsx prisma/seed/seed.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "dotenv": "16.4.5",
    "express": "4.21.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@cspell/dict-lorem-ipsum": "4.0.3",
    "@eslint/js": "9.15.0",
    "@snaplet/copycat": "5.1.0",
    "@snaplet/seed": "0.98.0",
    "@types/eslint__js": "8.42.3",
    "@types/express": "5.0.0",
    "@types/node": "22.9.0",
    "cspell": "8.16.0",
    "eslint": "9.14.0",
    "eslint-config-prettier": "9.1.0",
    "globals": "15.12.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "prisma": "5.22.0",
    "tsx": "4.19.2",
    "typescript": "5.6.3",
    "typescript-eslint": "8.14.0"
  },
  "@snaplet/seed": {
    "config": "prisma/seed/seed.config.ts"
  },
  "lint-staged": {
    "*.{json,html}": [
      "prettier --write",
      "cspell --no-must-find-files"
    ],
    "*.{md,yaml,yml}": [
      "cspell --no-must-find-files"
    ],
    "*.{ts,js}(x)?": [
      "eslint --fix",
      "prettier --write",
      "cspell --no-must-find-files"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/seed/seed.ts"
  },
  "volta": {
    "node": "22.11.0",
    "npm": "10.9.0"
  }
}
